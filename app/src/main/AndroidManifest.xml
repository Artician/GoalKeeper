<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.goalkeeper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoalKeeper">
        <activity android:name=".AppTaskCreate"></activity>
        <activity android:name=".AppTaskView" />
        <activity android:name=".AppGoalsBuilder" />
        <activity android:name=".AppGoalsYear" />
        <activity android:name=".AppGoalsMonth" />
        <activity android:name=".AppGoalsWeek" />
        <activity android:name=".AppGoalsDay" />
        <activity android:name=".AppGoalsMaster" />

        <service
            android:name=".AppNotification"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".AppWebConnect"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".AppIO"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".AppGoalsView" />
        <activity android:name=".AppSettings" />
        <activity android:name=".AppMonth" />
        <activity android:name=".AppWeek" />
        <activity android:name=".AppDay" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>